module: satosa.backends.openid_connect.OpenIdBackend
name: openid_connect
plugin: BackendModulePlugin

config:
  acr_values: [PASSWORD]
  authz_page: <name>
  client:
    behaviour:
      response_type: code
      scope: [openid, profile, email, address, phone]
    client_info:
      application_name:
      application_type: web
      contacts: []
      post_logout_redirect_uris: [<base_url>/<name>/logout]
      redirect_uris: [<base_url>/<name>/sauthz_cb]
      response_types: [code]
      subject_type: pairwise
    userid:
  op_url:
  state_id:
  verify_ssl: false
  op_info:
    contact_person:
    - contact_type:
        email_address: []
        given_name:
        sur_name:
    - contact_type:
        email_address: []
        given_name:
    organization:
      display_name:
        - ["", en]
      name:
        - ["", se]
        - ["", en]
      url:
        - ["", en]
        - ["", se]
    ui_info:
      description:
        - ["", en]
      display_name:
        - ["", en]
      logo:
        - {width: '', height: '', lang: en, image: ''}
